{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "api/*": [
        "src/api/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "navigation/*": [
        "src/navigation/*"
      ],
      "screens/*": [
        "src/screens/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "types/*": [
        "src/types/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
    }
  }
}